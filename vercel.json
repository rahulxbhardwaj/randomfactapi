{
  "version": 2,
  "builds": [
    {
      "src": "index.js", // Entry point of your Node.js application
      "use": "@vercel/node" // Specifies the builder to use for Node.js projects
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js" // The entry point of your application
    }
  ]
}
